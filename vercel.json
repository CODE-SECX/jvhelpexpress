{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/admin-styles.css",
      "dest": "/public/admin/admin-styles.css"
    },
    {
      "src": "/admin-auth.js",
      "dest": "/public/admin/admin-auth.js"
    },
    {
      "src": "/admin/dashboard",
      "dest": "/public/admin/dashboard.html"
    },
    {
      "src": "/admin/dashboard.html",
      "dest": "/public/admin/dashboard.html"
    },
    {
      "src": "/admin/(.*\\.(js|css|html))",
      "dest": "/public/admin/$1"
    },
    {
      "src": "/admin/?.*",
      "dest": "/public/admin/index.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}